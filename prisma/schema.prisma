generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Booking {
  id        String   @id @default(uuid())
  name      String
  email     String
  date      DateTime
  time      String
 
  createdAt DateTime @default(now())
   reminderSent  Boolean  @default(false)
}
